function button1(){
    document.getElementById("index").style.visibility = "hidden";
    document.getElementById("greenIndex").style.visibility = "hidden";
    document.getElementById("orangeIndex").style.visibility = "hidden";
    document.getElementById("redIndex").style.visibility = "visible";
}
function button2(){
    document.getElementById("index").style.visibility = "hidden";
    document.getElementById("redIndex").style.visibility = "hidden";
    document.getElementById("orangeIndex").style.visibility = "hidden";
    document.getElementById("greenIndex").style.visibility = "visible";
}
function button3(){
    document.getElementById("index").style.visibility = "hidden";
    document.getElementById("redIndex").style.visibility = "hidden";
    document.getElementById("greenIndex").style.visibility = "hidden";
    document.getElementById("orangeIndex").style.visibility = "visible";
}
function button4(){
    document.getElementById("redIndex").style.visibility = "hidden";
    document.getElementById("greenIndex").style.visibility = "hidden";
    document.getElementById("orangeIndex").style.visibility = "hidden";
    document.getElementById("index").style.visibility = "visible";
